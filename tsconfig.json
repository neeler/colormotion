{
	"compilerOptions": {
		"target": "es2016",
		"module": "ESNext",
		"moduleResolution": "bundler",
		"declaration": true,
		"outDir": "./dist",
		"strict": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"**/*.test.ts"
	]
}
